
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="author" content="Jeff Wrathall" />
    <meta name="robots" content="index,follow" />
     <meta name="description" content="Freelance web developer located in Munich Germany. My focus is on .net framework and front end development  but I can also develop with php and ruby on rails." />
    <meta name="keywords" content="Jeff Wrathall, Freelance web developer, .Net development, Ruby on Rails applications, Wordpress development, PHP development, web applications, Front end web development, MVC, javascript,javascript frameworks, Angular.js, Backbone.js, CSS, LESS, responsive design, mobile web development, web technology" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Assets/Styles/reset.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Styles/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Styles/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Assets/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Scripts/jquery.easing.1.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Scripts/angular.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Scripts/bootstrap.min.js")" type="text/javascript"></script>
   
</head>

    <body>
        <div id="Wrapper">
            <div>
                <header>
                    @Html.Partial("Partial/Header")
                </header> 
            </div>
            <div id="Content">
                <div id="Tagline">
                    <div class="row">
                        <div class="span5" id="site-title">
                            <div style="text-align:left;margin:25px 0 0 10px;">
                                <a href="@Url.Action("Index","Home")" target="_self">
                                    <img src= "@Url.Content("~/Assets/Images/Title.png")" alt="jeffwrathall.com"/>
                                </a>  
                            </div>
                        </div>
                        <div class="span5">
                            <div style="margin-top:14px;">
                                <div id="NavOver">
                                    <div id="NavOverText"></div>
                                </div>
                                <div id="Nav">
                                    <ul>
                                        <li>
                                            <a href="@Url.Action("Index","Home")" target="_self" rel="Home"></a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("FrontEnd","Skills")" target="_self" rel="Front End"></a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("WebApplications","Skills")" target="_self" rel="Applications"></a>
                                        </li>

                                        <li>
                                            <a href="@Url.Action("Index","About")" target="_self" rel="About Me"></a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Index","PastWork")" target="_self" rel="Past Work"></a>
                                        </li>
                                    </ul> 
                                </div>
                                    
                            </div>
                        </div>
                    </div>
                </div>
               
                @RenderBody()
            </div>
        </div>
        <div>
            <footer>
                @Html.Partial("Partial/Footer")
            </footer>
        </div>
    </body>
    <script src="@Url.Content("~/Assets/Scripts/site.js")" type="text/javascript"></script>
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-38876911-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

</script>
</html>